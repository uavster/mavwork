 130 00bc 4040C403 		BICEQ   r4, r4, #F_Bit
 131 00c0 40408413 		ORRNE   r4, r4, #F_Bit
 132 00c4 04F061E1 		MSR     SPSR_c, r4
 133 00c8 FFFFFFEA 		B       EndofSWI
